#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1392670 .scope module, "test_sigmoid" "test_sigmoid" 2 1;
 .timescale 0 0;
v0x13b2a30_0 .var "clk", 0 0;
v0x13b2af0_0 .var/i "i", 31 0;
v0x13b2bb0_0 .var "reset", 0 0;
v0x13b2cb0_0 .var/s "x", 7 0;
v0x13b2d80_0 .net "y", 7 0, v0x13b27a0_0;  1 drivers
E_0x139a230 .event negedge, v0x1392970_0;
E_0x139a400 .event posedge, v0x1392970_0;
S_0x13927f0 .scope module, "instance_piecewise" "sigmoid_piecewise" 2 8, 3 1 0, S_0x1392670;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 8 "x_in"
    .port_info 3 /OUTPUT 8 "y_out"
P_0x1399560 .param/l "OFFSET_1" 1 3 14, +C4<00000101>;
P_0x13995a0 .param/l "OFFSET_2" 1 3 18, +C4<00100000>;
P_0x13995e0 .param/l "OFFSET_3" 1 3 22, +C4<01000101>;
P_0x1399620 .param/l "OFFSET_4" 1 3 26, +C4<10111011>;
P_0x1399660 .param/l "OFFSET_5" 1 3 30, +C4<11100001>;
P_0x13996a0 .param/l "SHIFT_1" 1 3 13, +C4<00000010>;
P_0x13996e0 .param/l "SHIFT_2" 1 3 17, +C4<00000001>;
P_0x1399720 .param/l "SHIFT_3" 1 3 21, +C4<00000001>;
P_0x1399760 .param/l "SHIFT_4" 1 3 25, +C4<00000001>;
P_0x13997a0 .param/l "SHIFT_5" 1 3 29, +C4<00000010>;
v0x1392970_0 .net "clk", 0 0, v0x13b2a30_0;  1 drivers
v0x13b25f0_0 .net "reset", 0 0, v0x13b2bb0_0;  1 drivers
v0x13b26b0_0 .net/s "x_in", 7 0, v0x13b2cb0_0;  1 drivers
v0x13b27a0_0 .var "y_out", 7 0;
v0x13b2880_0 .var "y_s", 7 0;
E_0x1399b90 .event edge, v0x13b26b0_0;
E_0x1392c30/0 .event negedge, v0x13b25f0_0;
E_0x1392c30/1 .event posedge, v0x1392970_0;
E_0x1392c30 .event/or E_0x1392c30/0, E_0x1392c30/1;
    .scope S_0x13927f0;
T_0 ;
    %wait E_0x1392c30;
    %load/vec4 v0x13b25f0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x13b27a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x13b2880_0;
    %assign/vec4 v0x13b27a0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x13927f0;
T_1 ;
    %wait E_0x1399b90;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %cmpi/s 4294967168, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_1.0, 5;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x13b2880_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 127, 0, 32;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %cmp/s;
    %flag_or 5, 4;
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x13b2880_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %cmpi/s 4294967232, 0, 32;
    %jmp/0xz  T_1.4, 5;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %addi 128, 0, 32;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr/s 4;
    %addi 5, 0, 32;
    %pad/s 8;
    %store/vec4 v0x13b2880_0, 0, 8;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %cmpi/s 4294967264, 0, 32;
    %jmp/0xz  T_1.6, 5;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %addi 64, 0, 32;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr/s 4;
    %addi 32, 0, 32;
    %pad/s 8;
    %store/vec4 v0x13b2880_0, 0, 8;
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %cmpi/s 32, 0, 32;
    %jmp/0xz  T_1.8, 5;
    %load/vec4 v0x13b26b0_0;
    %addi 32, 0, 8;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %addi 69, 0, 8;
    %store/vec4 v0x13b2880_0, 0, 8;
    %jmp T_1.9;
T_1.8 ;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %cmpi/s 64, 0, 32;
    %jmp/0xz  T_1.10, 5;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %subi 32, 0, 32;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr/s 4;
    %addi 4294967227, 0, 32;
    %pad/s 8;
    %store/vec4 v0x13b2880_0, 0, 8;
    %jmp T_1.11;
T_1.10 ;
    %load/vec4 v0x13b26b0_0;
    %pad/s 32;
    %subi 64, 0, 32;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr/s 4;
    %addi 4294967265, 0, 32;
    %pad/s 8;
    %store/vec4 v0x13b2880_0, 0, 8;
T_1.11 ;
T_1.9 ;
T_1.7 ;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x1392670;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13b2a30_0, 0, 1;
T_2.0 ;
    %delay 1, 0;
    %load/vec4 v0x13b2a30_0;
    %inv;
    %store/vec4 v0x13b2a30_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x1392670;
T_3 ;
    %vpi_call 2 23 "$dumpfile", "test_sigmoid.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1392670 {0 0 0};
    %vpi_call 2 26 "$display", "Begin Reset" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13b2bb0_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x139a400;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13b2bb0_0, 0, 1;
    %vpi_call 2 30 "$display", "End reset" {0 0 0};
    %vpi_call 2 32 "$display", "Testing Sigmoid Module:" {0 0 0};
    %vpi_call 2 33 "$display", "  x\011PieceWise Linear" {0 0 0};
    %wait E_0x139a230;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x13b2af0_0, 0, 32;
T_3.2 ;
    %load/vec4 v0x13b2af0_0;
    %cmpi/s 256, 0, 32;
    %jmp/0xz T_3.3, 5;
    %load/vec4 v0x13b2af0_0;
    %subi 128, 0, 32;
    %pad/s 8;
    %store/vec4 v0x13b2cb0_0, 0, 8;
    %wait E_0x139a230;
    %vpi_call 2 38 "$display", "%d\011\011%d\011\011", v0x13b2cb0_0, v0x13b2d80_0 {0 0 0};
    %load/vec4 v0x13b2af0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x13b2af0_0, 0, 32;
    %jmp T_3.2;
T_3.3 ;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
